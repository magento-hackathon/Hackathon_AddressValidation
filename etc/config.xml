<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <checkout>
            <address_validation>
                <enabled>1</enabled>
                <adapters>
                    <mediact_dummy>
                        <label>MediaCT Dummy</label>
                        <class>Hackathon\AddressValidation\Lookup\Adapter\MediaCTAdapter</class>
                    </mediact_dummy>
                </adapters>
                <max_suggestions>5</max_suggestions>
            </address_validation>
        </checkout>
    </default>
</config>
